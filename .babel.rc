
{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "next/babel"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic"
      }
    ],
    ["@babel/plugin-proposal-class-properties"]
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  }
}